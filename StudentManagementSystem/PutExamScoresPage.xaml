<Page x:Class="StudentManagementSystem.Pages.PutExamScoresPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      Title="Put Exam Scores">
    <StackPanel>
        <TextBox x:Name="StudentIdTextBox" Text="Enter Student ID"/>
        <TextBox x:Name="SubjectTextBox" Text="Enter Subject"/>
        <TextBox x:Name="ScoreTextBox" Text="Enter Score"/>
        <Button Content="Put Exam Score" Click="PutExamScore_Click"/>
    </StackPanel>
</Page>
